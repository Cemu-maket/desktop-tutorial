        ];

        const typingQuestions = [
            { question: "ã“ã‚“ã«ã¡ã¯", meaning: "Hello", answers: ["konnichiwa", "ã“ã‚“ã«ã¡ã¯"] },
            { question: "ã‚ã‚ŠãŒã¨ã†", meaning: "Thank you", answers: ["arigatou", "ã‚ã‚ŠãŒã¨ã†"] },
            { question: "ãŠã¯ã‚ˆã†", meaning: "Good morning", answers: ["ohayou", "ãŠã¯ã‚ˆã†"] },
            { question: "ã•ã‚ˆã†ãªã‚‰", meaning: "Goodbye", answers: ["sayounara", "ã•ã‚ˆã†ãªã‚‰"] },
            { question: "ã™ã¿ã¾ã›ã‚“", meaning: "Excuse me", answers: ["sumimasen", "ã™ã¿ã¾ã›ã‚“"] }
        ];

        const voiceQuestions = [
            { question: "ã‚ã‚ŠãŒã¨ã†", meaning: "Thank you" },
            { question: "ã“ã‚“ã«ã¡ã¯", meaning: "Hello" },
            { question: "ãŠã¯ã‚ˆã†", meaning: "Good morning" },
            { question: "ã•ã‚ˆã†ãªã‚‰", meaning: "Goodbye" },
            { question: "ã™ã¿ã¾ã›ã‚“", meaning: "Excuse me" }
        ];

        // State
        let currentCardIndex = 0;
        let currentTypingIndex = 0;
        let currentVoiceIndex = 0;
        let isCardFlipped = false;
        let isRecording = false;
        let typingCorrect = 0;
        let typingTotal = 0;

        // Tab functionality
        document.querySelectorAll('.tab-btn').forEach(btn => {
            btn.addEventListener('click', () => {
                const tabId = btn.dataset.tab;
                
                // Update active tab button
                document.querySelectorAll('.tab-btn').forEach(b => {
                    b.classList.remove('active', 'border-white', 'text-white');
                    b.classList.add('border-transparent', 'text-white/80');
                });
                btn.classList.add('active', 'border-white', 'text-white');
                btn.classList.remove('border-transparent', 'text-white/80');
                
                // Show active tab content
                document.querySelectorAll('.tab-content').forEach(content => {
                    content.classList.remove('active');
                });
                document.getElementById(tabId).classList.add('active');
            });
        });

        // Flashcard functionality
        function updateCard() {
            const card = deckN4[currentCardIndex];
            document.getElementById('card-word-text').textContent = card.word;
            document.getElementById('card-word-reading').textContent = card.reading;
            document.getElementById('card-meaning-text').textContent = card.meaning;
            document.getElementById('card-example').textContent = card.example;
            document.getElementById('card-counter').textContent = currentCardIndex + 1;
            document.getElementById('total-cards').textContent = deckN4.length;
            
            // Reset flip state
            document.getElementById('flashcard').classList.remove('flipped');
            isCardFlipped = false;
        }

        function nextCard() {
            currentCardIndex = (currentCardIndex + 1) % deckN4.length;
            updateCard();
        }

        function previousCard() {
            currentCardIndex = (currentCardIndex - 1 + deckN4.length) % deckN4.length;
            updateCard();
        }

        function playPronunciation() {
            // Simulate pronunciation playback
            alert('ç™ºéŸ³ã‚’å†ç”Ÿä¸­... (å®Ÿéš›ã®å®Ÿè£…ã§ã¯éŸ³å£°APIã‚’ä½¿ç”¨)');
        }

        function toggleFavorite() {
            alert('ãŠæ°—ã«å…¥ã‚Šã«è¿½åŠ ã—ã¾ã—ãŸï¼');
        }

        // Card flip functionality
        document.getElementById('flashcard').addEventListener('click', () => {
            const card = document.getElementById('flashcard');
            card.classList.toggle('flipped');
            isCardFlipped = !isCardFlipped;
        });

        // Typing functionality
        function updateTypingQuestion() {
            const question = typingQuestions[currentTypingIndex];
            document.getElementById('typing-question').textContent = question.question;
            document.getElementById('typing-meaning').textContent = question.meaning;
            document.getElementById('typing-counter').textContent = currentTypingIndex + 1;
            document.getElementById('typing-total').textContent = typingQuestions.length;
            document.getElementById('typing-input').value = '';
            document.getElementById('typing-input').focus();
        }

        function checkTypingAnswer() {
            const input = document.getElementById('typing-input');
            const question = typingQuestions[currentTypingIndex];
            const userAnswer = input.value.trim().toLowerCase();
            
            const isCorrect = question.answers.some(answer => 
                answer.toLowerCase() === userAnswer
            );
            
            if (isCorrect) {
                // Success animation
                input.parentElement.parentElement.classList.add('success-glow');
                typingCorrect++;
                
                setTimeout(() => {
                    input.parentElement.parentElement.classList.remove('success-glow');
                    nextTypingQuestion();
                }, 1000);
            } else if (userAnswer.length > 0) {
                // Error animation
                input.parentElement.parentElement.classList.add('error-shake');
                
                setTimeout(() => {
                    input.parentElement.parentElement.classList.remove('error-shake');
                }, 500);
            }
            
            typingTotal++;
            updateTypingAccuracy();
        }

        function nextTypingQuestion() {
            currentTypingIndex = (currentTypingIndex + 1) % typingQuestions.length;
            updateTypingQuestion();
        }

        function skipTypingQuestion() {
            nextTypingQuestion();
        }

        function resetTyping() {
            currentTypingIndex = 0;
            typingCorrect = 0;
            typingTotal = 0;
            updateTypingQuestion();
            updateTypingAccuracy();
        }

        function updateTypingAccuracy() {
            const accuracy = typingTotal > 0 ? Math.round((typingCorrect / typingTotal) * 100) : 100;
            document.getElementById('typing-accuracy').textContent = accuracy;
        }

        // Typing input event listener
        document.getElementById('typing-input').addEventListener('input', checkTypingAnswer);
        document.getElementById('typing-input').addEventListener('keypress', (e) => {
            if (e.key === 'Enter') {
                e.preventDefault();
                checkTypingAnswer();
            }
        });

        // Voice functionality
        function updateVoiceQuestion() {
            const question = voiceQuestions[currentVoiceIndex];
            document.getElementById('voice-question').textContent = question.question;
            document.getElementById('voice-meaning').textContent = question.meaning;
            document.getElementById('voice-feedback').textContent = 'ãƒã‚¤ã‚¯ãƒœã‚¿ãƒ³ã‚’æŠ¼ã—ã¦ç™ºéŸ³ã—ã¦ãã ã•ã„';
            document.getElementById('voice-score').textContent = '--';
        }

        function toggleVoiceRecording() {
            const btn = document.getElementById('voice-btn');
            const waves = document.getElementById('voice-waves');
            const feedback = document.getElementById('voice-feedback');
            
            if (!isRecording) {
                // Start recording
                isRecording = true;
                btn.classList.remove('bg-red-500', 'hover:bg-red-600');
                btn.classList.add('bg-green-500', 'hover:bg-green-600');
                btn.textContent = 'â¹ï¸';
                waves.style.display = 'flex';
                feedback.textContent = 'éŒ²éŸ³ä¸­... ç™ºéŸ³ã—ã¦ãã ã•ã„';
                
                // Simulate recording for 3 seconds
                setTimeout(() => {
                    stopVoiceRecording();
                }, 3000);
            } else {
                stopVoiceRecording();
            }
        }

        function stopVoiceRecording() {
            const btn = document.getElementById('voice-btn');
            const waves = document.getElementById('voice-waves');
            const feedback = document.getElementById('voice-feedback');
            const score = document.getElementById('voice-score');
            
            isRecording = false;
            btn.classList.remove('bg-green-500', 'hover:bg-green-600');
            btn.classList.add('bg-red-500', 'hover:bg-red-600');
            btn.textContent = 'ğŸ¤';
            waves.style.display = 'none';
            
            // Simulate voice recognition result
            const randomScore = Math.floor(Math.random() * 30) + 70; // 70-100
            score.textContent = randomScore + '%';
            
            if (randomScore >= 80) {
                feedback.textContent = 'ç´ æ™´ã‚‰ã—ã„ç™ºéŸ³ã§ã™ï¼';
                setTimeout(() => {
                    nextVoiceQuestion();
                }, 2000);
            } else {
                feedback.textContent = 'ã‚‚ã†ä¸€åº¦è©¦ã—ã¦ã¿ã¾ã—ã‚‡ã†ã€‚å£ã®å½¢ã‚’æ„è­˜ã—ã¦ãã ã•ã„ã€‚';
            }
        }

        function nextVoiceQuestion() {
            currentVoiceIndex = (currentVoiceIndex + 1) % voiceQuestions.length;
            updateVoiceQuestion();
        }

        // Admin functionality
        function exportCSV() {
            const csvContent = "data:text/csv;charset=utf-8," + 
                "ã‚«ãƒ¼ãƒ‰,èª­ã¿æ–¹,æ„å‘³,æ­£è§£ç‡\n" +
                deckN4.map(card => 
                    `${card.word},${card.reading},${card.meaning},${Math.floor(Math.random() * 30) + 70}%`
                ).join("\n");
            
            const encodedUri = encodeURI(csvContent);
            const link = document.createElement("a");
            link.setAttribute("href", encodedUri);
            link.setAttribute("download", "å­¦ç¿’ãƒ‡ãƒ¼ã‚¿.csv");
            document.body.appendChild(link);
            link.click();
            document.body.removeChild(link);
        }

        function exportExcel() {
            alert('Excelå½¢å¼ã§ã®ã‚¨ã‚¯ã‚¹ãƒãƒ¼ãƒˆæ©Ÿèƒ½ã‚’å®Ÿè¡Œä¸­...\n(å®Ÿéš›ã®å®Ÿè£…ã§ã¯é©åˆ‡ãªãƒ©ã‚¤ãƒ–ãƒ©ãƒªã‚’ä½¿ç”¨)');
        }

        // Touch/swipe support for mobile
        let touchStartX = 0;
        let touchStartY = 0;

        document.addEventListener('touchstart', (e) => {
            touchStartX = e.touches[0].clientX;
            touchStartY = e.touches[0].clientY;
        });

        document.addEventListener('touchend', (e) => {
            if (!touchStartX || !touchStartY) return;
            
            const touchEndX = e.changedTouches[0].clientX;
            const touchEndY = e.changedTouches[0].clientY;
            
            const diffX = touchStartX - touchEndX;
            const diffY = touchStartY - touchEndY;
            
            // Only handle horizontal swipes on cards tab
            if (document.getElementById('cards').classList.contains('active')) {
                if (Math.abs(diffX) > Math.abs(diffY) && Math.abs(diffX) > 50) {
                    if (diffX > 0) {
                        nextCard(); // Swipe left = next card
                    } else {
                        previousCard(); // Swipe right = previous card
                    }
                }
            }
            
            touchStartX = 0;
            touchStartY = 0;
        });

        // Keyboard support
        document.addEventListener('keydown', (e) => {
            if (document.getElementById('cards').classList.contains('active')) {
                if (e.key === 'ArrowLeft') {
                    previousCard();
                } else if (e.key === 'ArrowRight') {
                    nextCard();
                } else if (e.key === ' ') {
                    e.preventDefault();
                    document.getElementById('flashcard').click();
                }
            }
        });

        // Initialize
        updateCard();
        updateTypingQuestion();
        updateVoiceQuestion();
    </script>
